<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:popups="clr-namespace:Lastik.ViewModels.Popups"
                    xmlns:popups1="clr-namespace:Lastik.Views.Popups"
                    xmlns:b="http://schemas.microsoft.com/xaml/behaviors">
   

    <DataTemplate DataType="{x:Type popups:LoaderViewModel}">
        <popups1:Loader/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:PaymentPopupViewModel}">
        <popups1:PaymentPopupView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:PasswordPopupViewModel}">
        <popups1:PasswordPopupView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:ServerErrorPopupViewModel}">
        <popups1:ServerErrorPopupView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:CalendarPopupViewModel}">
        <popups1:CalendarPopupView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:FiltersPopupViewModel}">
        <popups1:FiltersPopupView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:PlaceViewPopupViewModel}">
        <popups1:PlaceViewPopup/>
    </DataTemplate>
</ResourceDictionary>